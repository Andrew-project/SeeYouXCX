.header-title {
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 9999;
}
.is-back-title {
  background-color: #fff;
  color: #666;
}
.font-white {
  color: #fff;
}
.status-bar {
  background-color: #fff;
}
.right-icon {
  width: 88rpx;
  float: right;
}
.title {
  width: 574rpx;
  text-align: center;
  float: left;
}
.title-bar {
  width: 100%;
  overflow: hidden;
}
.border-bottom-1px {
  border-bottom: 1px solid #e8e8e8;
}
.left-icon {
  width: 88rpx;
  float: left;
}
/*箭头部分*/
.navigator {
  width: 18rpx;
  height: 32rpx;
  float: left;
  margin-top: 28rpx;
  margin-left: 35rpx;
}
.navigator:after {
  content: "";
  display: block;
  width: 20rpx;
  height: 20rpx;
  border-right: 2rpx solid #3c3c3c;
  border-top: 2rpx solid #3c3c3c;
  -webkit-transform: rotate(225deg);
  /*箭头方向可以自由切换角度*/
  transform: rotate(225deg);
}
.navigator-white {
  width: 18rpx;
  height: 32rpx;
  float: left;
  margin-top: 28rpx;
  margin-left: 35rpx;
}
.navigator-white:after {
  content: "";
  display: block;
  width: 20rpx;
  height: 20rpx;
  border-right: 2rpx solid #ffffff;
  border-top: 2rpx solid #ffffff;
  -webkit-transform: rotate(225deg);
  /*箭头方向可以自由切换角度*/
  transform: rotate(225deg);
}
